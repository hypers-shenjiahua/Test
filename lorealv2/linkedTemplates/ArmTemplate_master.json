{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "lorealv2"
		},
		"allpurposedcluster_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'allpurposedcluster'"
		},
		"lorealv4_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'lorealv4'"
		},
		"allpurposedcluster_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1115-100811-3wsmdq3m"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "lorealv2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"allpurposedcluster_accessToken": {
						"value": "[parameters('allpurposedcluster_accessToken')]"
					},
					"lorealv4_accessToken": {
						"value": "[parameters('lorealv4_accessToken')]"
					},
					"allpurposedcluster_properties_typeProperties_existingClusterId": {
						"value": "[parameters('allpurposedcluster_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}